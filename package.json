		"lodash": "^4.17.15",
		"mongoose": "^5.9.18",
		"nodemon": "^1.19.4",
		"request": "^2.88.2"
	},
	"devDependencies": {
		"cors": "^2.8.5",
		"eslint": "^6.8.0",
		"eslint-config-airbnb-base": "^14.1.0",
		"eslint-config-prettier": "^6.11.0",
		"eslint-plugin-import": "^2.21.1",
		"eslint-plugin-jest": "^23.13.2",
		"eslint-plugin-prettier": "^3.1.3",
		"jest": "^26.0.1",
		"jest-sonar-reporter": "^2.0.0",
